{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api-docs", "dest": "/api/index.js" },
    { "src": "/users(.*)", "dest": "/api/index.js" },
    { "src": "/instructors(.*)", "dest": "/api/index.js" },
    { "src": "/dancedata(.*)", "dest": "/api/index.js" },
    { "src": "/danceclasses(.*)", "dest": "/api/index.js" },
    { "src": "/danceforms(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}